{
  "name": "css-zero-beautify",
  "description": "Friendly and forgiving CSS/SASS/LESS beautifier",
  "version": "2.0.3",
  "scripts": {
    "build": "babel src --out-dir build",
    "prepublishOnly": "npm run build",
    "test": "jest",
    "flow": "flow"
  },
  "keywords": ["beautify", "prettify", "css", "sass", "scss", "less"],
  "author": "Matthew Holloway",
  "license": "GPL-3.0",
  "repository": {
    "type": "git",
    "url": "https://github.com/holloway/xml-zero.js/"
  },
  "//": [
    "babel-cli: needed for building",
    "babel-register: needed for testing via Jest"
  ],
  "devDependencies": {
    "babel-cli": "^6.24.1",
    "babel-core": "^6.26.0",
    "babel-jest": "^22.4.1",
    "babel-plugin-transform-class-properties": "^6.24.1",
    "babel-plugin-transform-object-rest-spread": "^6.26.0",
    "babel-preset-env": "^1.6.0",
    "babel-preset-flow": "^6.23.0",
    "babel-register": "^6.26.0",
    "flow-bin": "^0.55.0",
    "jest": "^21.0.2"
  },
  "env": {
    "test": {
      "presets": ["env"]
    }
  },
  "main": "./build/index.js",
  "babel": {
    "presets": [
      "flow",
      [
        "env",
        {
          "targets": {
            "node": "3"
          }
        }
      ]
    ],
    "plugins": [["transform-object-rest-spread"]]
  },
  "dependencies": {
    "css-zero-lexer": "^1.0.0"
  }
}
