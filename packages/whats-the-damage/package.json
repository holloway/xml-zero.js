{
  "name": "whats-the-damage",
  "description":
    "What's the damage? ...of running that script in CPU and memory usage",
  "version": "1.0.2",
  "keywords": [
    "benchmark",
    "cpu",
    "memory",
    "ram",
    "damage",
    "flowtype",
    "typing"
  ],
  "scripts": {
    "build": "babel src --out-dir build",
    "prepublishOnly": "npm run build",
    "test": "jest",
    "flow": "flow"
  },
  "author": "Matthew Holloway",
  "license": "GPL-3.0",
  "repository": {
    "type": "git",
    "url":
      "https://github.com/holloway/xml-zero.js/tree/master/packages/whats-the-damage"
  },
  "//": [
    "babel-cli: needed for building",
    "babel-register: needed for testing via Jest"
  ],
  "devDependencies": {
    "babel-cli": "^6.24.1",
    "babel-plugin-transform-object-rest-spread": "^6.26.0",
    "babel-preset-env": "^1.6.0",
    "babel-register": "^6.26.0",
    "flow-bin": "^0.55.0",
    "jest": "^21.0.2"
  },
  "module": "./src/index.js",
  "main": "./build/index.js",
  "babel": {
    "presets": [
      "flow",
      [
        "env",
        {
          "targets": {
            "node": "4"
          }
        }
      ]
    ],
    "plugins": [
      [
        "transform-object-rest-spread",
        {
          "useBuiltIns": true
        }
      ]
    ]
  },
  "dependencies": {
    "pidusage": "^1.1.6",
    "stats-lite": "^2.1.0"
  }
}
